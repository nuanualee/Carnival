<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="RoundedSquare"> //FOR ROUNDED
        <item name = "cornerSize" > 10%</item> //percentage is roundness
    </style>

    <style name="MoreRound"> //FOR ROUNDED
        <item name = "cornerSize" > 50%</item> //percentage is roundness
    </style>

    <style name="Rounded"> //FOR ROUNDED
